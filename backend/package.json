{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.ts'",
    "test:unit": "jest --testMatch='**/__tests__/{controllers,services,middleware}/**/*.test.ts'",
    "security:audit": "npx tsx src/scripts/security/audit.ts",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "migrate:status": "knex migrate:status --knexfile knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts -x ts",
    "seed:make": "knex seed:make --knexfile knexfile.ts -x ts",
    "seed:run": "knex seed:run --knexfile knexfile.ts"
  },
  "dependencies": {
    "@sentry/node": "^10.23.0",
    "@sentry/profiling-node": "^10.23.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/bull": "^3.15.9",
    "@types/ioredis": "^4.28.10",
    "@types/pg": "^8.15.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bottleneck": "^2.19.5",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.25.12",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "openai": "^6.8.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "prom-client": "^15.1.3",
    "redis": "^5.9.0",
    "rss-parser": "^3.13.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@esbuild/darwin-arm64": "^0.26.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
